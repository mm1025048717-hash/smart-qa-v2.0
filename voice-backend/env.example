# DeepSeek API Key (用于 LLM)
DEEPSEEK_API_KEY=your_deepseek_api_key_here
VITE_DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Deepgram API Key (用于语音转文字 STT)
DEEPGRAM_API_KEY=your_deepgram_api_key_here

# TTS 服务选择 (deepgram, cartesia, elevenlabs, piper, openai)
# 推荐使用 deepgram（与 STT 共用 API Key，无需额外配置）
TTS_SERVICE=deepgram

# OpenAI API Key (用于文字转语音 TTS，如果 TTS_SERVICE=openai)
# 注意：可以使用其他 TTS 服务，如 Cartesia, ElevenLabs, Piper 等
OPENAI_API_KEY=your_openai_api_key_here

# Cartesia API Key (如果 TTS_SERVICE=cartesia)
CARTESIA_API_KEY=your_cartesia_api_key_here
CARTESIA_VOICE_ID=71a7ad14-091c-4e8e-a314-022ece01c121

# ElevenLabs API Key (如果 TTS_SERVICE=elevenlabs)
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
ELEVENLABS_VOICE_ID=21m00Tcm4TlvDq8ikWAM

# WebSocket 服务器配置
WS_HOST=localhost
WS_PORT=8765

